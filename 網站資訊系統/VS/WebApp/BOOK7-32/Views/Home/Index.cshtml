@model IEnumerable<BOOK7_32.Models.tEmployee>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fEmpId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fDepId)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fEmpId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fDepId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.fEmpId }) |
                @Html.ActionLink("Details", "Details", new { id = item.fEmpId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.fEmpId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
